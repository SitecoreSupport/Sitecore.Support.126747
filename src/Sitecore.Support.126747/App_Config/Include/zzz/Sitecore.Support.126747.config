<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:set="http://www.sitecore.net/xmlconfig/set/" xmlns:role="http://www.sitecore.net/xmlconfig/role/">
  <sitecore role:require="Standalone OR ContentManagement OR ContentDelivery OR Processing OR Reporting">
    <pipelines>
      <healthMonitor>
        <processor type="Sitecore.Pipelines.HealthMonitor.HealthMonitor, Sitecore.Kernel" method="DumpRenderingsStatistics">
          <patch:attribute name="type">Sitecore.Support.Pipelines.HealthMonitor.HealthMonitor, Sitecore.Support.126747</patch:attribute>
        </processor>
      </healthMonitor>
    </pipelines>
  </sitecore>
</configuration>